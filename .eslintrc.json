{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "nonblock-statement-body-position": "off",
    "curly": ["error", "multi"]
  }
}
